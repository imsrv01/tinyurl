{
    "family": "tinyurl",
    "containerDefinitions": [
        {
            "image": "%REPOSITORY_URI%:%BUILD_NUMBER%",
            "name": "tinyurl",
            "cpu": 10,
            "memory": 512,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 5000,
                    "hostPort": 8081
                }
            ]
        }
    ]
}
